<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.metapp.indianapps">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon1"
        android:label="Install Indian Apps"
        android:roundIcon="@mipmap/appicon1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".category"></activity>
        <activity android:name=".shoppin"></activity>
        <activity android:name=".social" />
        <activity android:name=".productivity" />
        <activity android:name=".tiktok" />
        <activity android:name=".splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>